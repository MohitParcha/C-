#include<iostream>
using namespace std;

int main(int argc, char const *argv[])
{


    int amount1,amount2;
    cout<<"Enter the amount of money given by mom\n";
    cin>>amount1;


    cout<<"Enter the amount of money given by dad\n";
    cin>>amount2;


    int sum=amount1+amount2;
    cout<<"Sum is "<<sum;

    return 0;
}
